<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DocMainView">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="16dp"
        android:src="@drawable/circle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/dr_richard"
        android:layout_width="213dp"
        android:layout_height="58dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="32dp"
        android:gravity="top"
        android:text="Biira Clare"
        android:textStyle="bold"
        android:textAppearance="@style/dr_richard"
        app:layout_constraintEnd_toStartOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.736"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/kampala_hos"
        android:layout_width="205dp"
        android:layout_height="30dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="-5dp"
        android:gravity="top"
        android:text="EDD 23rd/11/2021"

        android:textAppearance="@style/kampala_hos"
        app:layout_constraintEnd_toEndOf="@+id/dr_richard"
        app:layout_constraintHorizontal_bias="0.631"
        app:layout_constraintStart_toStartOf="@+id/dr_richard"
        app:layout_constraintTop_toBottomOf="@+id/dr_richard" />

    <TextView
        android:id="@+id/personal_de"
        android:layout_width="228dp"
        android:layout_height="42dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="24dp"
        android:layout_marginTop="4dp"
        android:gravity="top"
        android:text="About Clare"
        android:textAppearance="@style/personal_de"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.065"
        app:layout_constraintStart_toStartOf="@+id/kampala_hos"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <androidx.cardview.widget.CardView
        android:id="@+id/card_view"
        android:layout_width="319dp"
        android:layout_height="201dp"
        android:layout_gravity="center"
        android:layout_marginTop="4dp"
        app:cardCornerRadius="4dp"
        app:layout_constraintEnd_toEndOf="@+id/personal_de"
        app:layout_constraintHorizontal_bias="0.255"
        app:layout_constraintStart_toStartOf="@+id/personal_de"
        app:layout_constraintTop_toBottomOf="@+id/personal_de">

        <TextView
            android:id="@+id/biira_clare"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:gravity="top"
            android:paddingStart="12dp"
            android:text="Second Pregancy"
            android:textAppearance="@style/biira_clare" />


    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/last_anc_vi"
        android:layout_width="267dp"
        android:layout_height="59dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="4dp"
        android:gravity="center_horizontal|top"
        android:textStyle="bold"
        android:text="@string/last_anc_vi"
        android:textAppearance="@style/last_anc_vi"
        app:layout_constraintEnd_toEndOf="@+id/card_view"
        app:layout_constraintStart_toStartOf="@+id/card_view"
        app:layout_constraintTop_toBottomOf="@+id/card_view" />

    <androidx.cardview.widget.CardView
        android:id="@+id/card_view2"
        android:layout_width="321dp"
        android:layout_height="210dp"
        android:layout_gravity="center"
        app:cardCornerRadius="4dp"
        app:layout_constraintEnd_toEndOf="@+id/last_anc_vi"
        app:layout_constraintHorizontal_bias="0.527"
        app:layout_constraintStart_toStartOf="@+id/last_anc_vi"
        app:layout_constraintTop_toBottomOf="@+id/last_anc_vi">

        <TextView
            android:id="@+id/biira_clare2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:gravity="top"
            android:paddingStart="12dp"
            android:text="23rd/10/2021"
            android:textAppearance="@style/biira_clare" />


    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>