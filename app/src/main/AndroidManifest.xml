<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.antentalcarekotlin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/log"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/log"
        android:supportsRtl="true"
        android:theme="@style/Theme.AntentalCarekotlin">
        <activity
            android:name=".MedicalDetail"
            android:exported="true" />
        <activity
            android:name=".PregnuncyDetails"
            android:exported="true" />
        <activity
            android:name=".SplashActivity"
            android:exported="true"
            android:theme="@style/AppTheme">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MotherView"
            android:exported="true" />
        <activity
            android:name=".SignUp"
            android:exported="true"
            android:theme="@style/AppTheme"/>
        <activity
            android:name=".LogIn"
            android:exported="true"
            android:theme="@style/AppTheme"
            />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/AppTheme">

        </activity>
    </application>

</manifest>